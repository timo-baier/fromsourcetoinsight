{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "fromsourcetoinsight"
		},
		"adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls'"
		},
		"passantenzaehlung_schoenbornstrasse_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.wuerzburg.de/api/records/1.0/search/?dataset=passantenzaehlung_schoenbornstrasse&q="
		},
		"adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tbintelligence.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fromsourcetoinsight_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_accountKey": {
						"value": "[parameters('adls_accountKey')]"
					},
					"passantenzaehlung_schoenbornstrasse_properties_typeProperties_url": {
						"value": "[parameters('passantenzaehlung_schoenbornstrasse_properties_typeProperties_url')]"
					},
					"adls_properties_typeProperties_url": {
						"value": "[parameters('adls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}